{
  "manifest_version": 2,
  "name": "Workable Unbias",
  "version": "1.0.0",
  "description": "Workable Rating Remover. Hides rating information in Workable.",
  "background": {
    "scripts": ["event_page.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Hide Ratings",
    "default_icon": {
      "128": "icons/workable-unbias.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.workable.com/backend/jobs/*"
      ],
      "css": [
        "hide_content.css"
      ]
    }
  ]
}
